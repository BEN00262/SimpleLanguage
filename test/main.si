fun fizzbuzz(number) {
    if number%3 == 0 {
        if number%5 == 0 {
            return "fizzbuzz"
        }
        return "buzz"
    } else if number%5 == 0 {
        return "fizz"
    }
}

for (def counter = 0; counter < 20; counter + 1) {
    if counter%3 == 0 {
        if counter%5 == 0 {
            print("fizzbuzz ====> {counter}")
        }
        print("buzz ====> {counter}")
    } else if counter%5 == 0 {
        print("fizz ====> {counter}")
    }  
}