letters = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"

fun IsLetter(char) {
    for (pos = 0; pos < len(letters); pos + 1) {
        if letters[pos] == char {
            return true
        }
    }

    return false
}

numbers = "0123456789"

fun IsNumber(number) {
    for (pos = 0; pos < len(letters); pos + 1) {
        if numbers[pos] == number {
            return true
        }
    }

    return false
}

VARIABLE = 0
NUMBER = 1

fun Lexer(code) {
    tokens = []

    for (pos = 0; pos < len(code); pos + 1) {
        lexeme = code[pos]

        if IsLetter(lexeme) {

            startPos = pos

            for (IsLetter(code[pos]) or IsNumber(code[pos])) {
                pos = pos + 1
            }

            append(tokens, [VARIABLE, code[startPos: pos]])

        } else if IsNumber(lexeme) {
            startPos = pos

            for (IsNumber(code[pos])) {
                pos = pos + 1
            }

            append(tokens, [NUMBER, code[startPos: pos]])
        }
    }

    return tokens
}