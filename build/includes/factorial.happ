expose fun factorial(number) {
    if number <= 1 {
        return 1
    }

    return number * factorial(number - 1)
}

expose fun fibonacci(number) {
    if number <= 0 {
        return 0
    }

    if number == 1 {
        return number
    }

    return fibonacci(number - 1) + fibonacci(number - 2)
}

expose def array = 200

expose fun sample() {
    return array + 89
}